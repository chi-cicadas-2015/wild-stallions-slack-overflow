<div class="container">
  <h1>Sign Up</h1>
  <form action="/users" method="post">
    <h3>Username</h3>
    <input type="text" name="user[name]">
    <h3>Email</h3>
    <input type="text" name="user[email]">
    <h3>Password</h3>
